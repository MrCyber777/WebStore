@model WebStore.Models.ProductTypes
@{
    ViewData["Title"] = "Details";
}

<h1 class="mt-1 text-info">Product Type Details</h1>
<div class="p-4 border rounded">      
    <div class="row form-group">
        <div class="col-2">
            <label asp-for="Name"></label>
        </div>
        <div class="col-5">
            <input asp-for="Name" class="form-control" disabled />
        </div>      
    </div>
    <br />


    <div class="form-group">
        <a asp-route-id="@Model.Id" class="btn btn-primary" asp-action="Edit">Edit</a>
        <a class="btn btn-success" asp-action="Index">Back to list</a>
    </div>
</div>


